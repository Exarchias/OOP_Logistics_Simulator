<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SimulationGUIController">
   <children>
      <Button fx:id="createSimulationBtn" layoutX="68.0" layoutY="284.0" mnemonicParsing="false" onMouseClicked="#clickedCreateSimulation" text="Create Simulation" />
      <Button fx:id="editSimulationBtn" layoutX="208.0" layoutY="284.0" mnemonicParsing="false" onMouseClicked="#clickedEditSimulation" text="Edit Simulation" />
      <Button fx:id="deleteSimulationBtn" layoutX="341.0" layoutY="284.0" mnemonicParsing="false" onMouseClicked="#clickedDeleteSimulation" text="Delete Simulation" />
      <ListView fx:id="simulationListView" layoutX="60.0" layoutY="37.0" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="logoutBtn" layoutX="454.0" layoutY="64.0" mnemonicParsing="false" onMouseClicked="#clickedOnLogout" text="logout" />
      <Button fx:id="goToDashboardBtn" layoutX="454.0" layoutY="125.0" mnemonicParsing="false" onMouseClicked="#clickedOnToDashboard" text="Go Back to Dashboard" />
   </children>
</AnchorPane>
